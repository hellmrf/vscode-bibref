{
  "publisher": "hellmrf",
  "name": "vscode-bibref",
  "displayName": "vscode-bibref",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@10.4.1",
  "description": "A VSCode Extension for BibLaTeX-friendly bibliography management",
  "author": "Heliton Martins <helitonmrf@gmail.com>",
  "license": "GPL-3.0-only",
  "homepage": "https://github.com/hellmrf/vscode-bibref#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/hellmrf/vscode-bibref"
  },
  "bugs": {
    "url": "https://github.com/hellmrf/vscode-bibref/issues"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "LICENSE.md",
    "dist/*",
    "res/*"
  ],
  "engines": {
    "vscode": "^1.97.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [{ "command": "bibref.hello", "category": "BibRef", "title": "Hello Command" }],
    "configuration": {
      "type": "object",
      "title": "bibref",
      "properties": {}
    }
  },
  "scripts": {
    "build": "tsdown src/index.ts --external vscode",
    "dev": "pnpm run build --watch --sourcemap",
    "prepare": "pnpm run update",
    "update": "vscode-ext-gen --output src/generated/meta.ts",
    "lint": "eslint .",
    "vscode:prepublish": "pnpm run build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && pnpm run publish"
  },
  "dependencies": {
    "@citation-js/core": "^0.7.18",
    "@citation-js/plugin-bibtex": "^0.7.18",
    "@citation-js/plugin-doi": "^0.7.18"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.2.1",
    "@types/node": "^22.13.4",
    "@types/vscode": "^1.97.0",
    "@vscode/vsce": "^3.2.2",
    "bumpp": "^10.0.3",
    "eslint": "^9.20.1",
    "esno": "^4.8.0",
    "pnpm": "^10.4.1",
    "reactive-vscode": "^0.2.10",
    "tsdown": "^0.9.6",
    "typescript": "^5.7.3",
    "vite": "^6.1.0",
    "vitest": "^3.0.5",
    "vscode-ext-gen": "^1.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
