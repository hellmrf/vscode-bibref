{
  "publisher": "coraldigital",
  "name": "bibref",
  "displayName": "BibRef",
  "version": "0.0.5",
  "private": true,
  "packageManager": "pnpm@10.4.1",
  "description": "A VSCode Extension for BibLaTeX-friendly bibliography management",
  "author": "Heliton Martins <helitonmrf@gmail.com>",
  "license": "GPL-3.0-only",
  "homepage": "https://github.com/hellmrf/vscode-bibref#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/hellmrf/vscode-bibref"
  },
  "bugs": {
    "url": "https://github.com/hellmrf/vscode-bibref/issues"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "LICENSE.md",
    "dist/*",
    "res/*"
  ],
  "engines": {
    "vscode": "^1.97.0"
  },
  "contributes": {
    "commands": [
      {
        "command": "bibref.fetchDOI",
        "category": "BibRef",
        "title": "Fetch from identifier..."
      }
    ],
    "configuration": {
      "type": "object",
      "title": "bibref",
      "properties": {}
    }
  },
  "scripts": {
    "build": "tsdown src/index.ts --external vscode",
    "dev": "pnpm run build --watch --sourcemap",
    "prepare": "pnpm run update",
    "update": "vscode-ext-gen --output src/generated/meta.ts",
    "lint": "eslint .",
    "vscode:prepublish": "pnpm run build",
    "publish": "vsce publish",
    "prepublish": "vsce publish --pre-release",
    "pack": "vsce package",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "tag": "bumpp"
  },
  "dependencies": {
    "@citation-js/core": "^0.7.18",
    "@citation-js/plugin-bibtex": "^0.7.18",
    "@citation-js/plugin-csl": "^0.7.18",
    "@citation-js/plugin-doi": "^0.7.18",
    "@citation-js/plugin-isbn": "^0.4.0",
    "@citation-js/plugin-software-formats": "^0.6.1",
    "@citation-js/plugin-wikidata": "^0.7.19"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.14.1",
    "@types/node": "^22.15.32",
    "@types/vscode": "^1.97.0",
    "@vscode/vsce": "^3.5.0",
    "bumpp": "^10.2.0",
    "eslint": "^9.29.0",
    "esno": "^4.8.0",
    "pnpm": "^10.12.1",
    "reactive-vscode": "^0.2.19",
    "tsdown": "^0.9.9",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.2.4",
    "vscode-ext-gen": "^1.0.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
